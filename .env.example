DOCKER_USERNAME=your_docker_username

FRONTEND_IMAGE_NAME=nextjs-app
FRONTEND_IMAGE_TAG=latest

BACKEND_IMAGE_NAME=nestjs-app
BACKEND_IMAGE_TAG=latest

NODE_ENV=development

FRONTEND_PORT=3000
BACKEND_PORT=4000

# Frontend

API_KEY=your_api_key_here

PROXY_ENDPOINT=http://nestjs-app:4000
PROXY_API_KEY=your_api_key_here

MEDIA_ENDPOINT=https://your-supabase-project.supabase.co/storage/v1/object/public/

AUTHOR_NAME=YourName
AUTHOR_PROFILE=https://github.com/yourprofile

NEXT_PUBLIC_FETCH_COUNT=30

NEXT_PUBLIC_SOCKET_HOSTNAME=localhost
NEXT_PUBLIC_SOCKET_PORT=8000
NEXT_PUBLIC_SOCKET_PATH=/socketcluster/
NEXT_PUBLIC_SOCKET_SECURE=false

# Backend

POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_strong_postgres_password
POSTGRES_DATABASE=myapp_db
POSTGRES_URI=postgresql://postgres:your_strong_postgres_password@postgres:5432/myapp_db

MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=your_strong_mongo_password
MONGO_DATABASE=myapp_db
MONGODB_URI=mongodb://admin:your_strong_mongo_password@mongodb:27017/myapp_db?authSource=admin

REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your_strong_redis_password

JWT_SECRET=your_jwt_secret_at_least_32_characters_long_random_string
JWT_EXPIRES_IN=15m
JWT_REFRESH_SECRET=your_refresh_secret_at_least_32_characters_long_random_string
JWT_REFRESH_EXPIRES_IN=7d

THROTTLE_TTL=60
THROTTLE_LIMIT=10

# Jenkins
JENKINS_WEB_PORT=8080
JENKINS_AGENT_PORT=50000

DOCKER_HOST=tcp://docker:2376
DOCKER_CERT_PATH=/certs/client
DOCKER_TLS_VERIFY=1
DOCKER_TLS_CERTDIR=/certs
DOCKER_STORAGE_DRIVER=overlay2

LOG_MAX_SIZE=10m
LOG_MAX_FILE=3

HEALTHCHECK_INTERVAL=30s
HEALTHCHECK_TIMEOUT=10s
HEALTHCHECK_RETRIES=5
HEALTHCHECK_START_PERIOD=90s